	<xsd:complexType name="removeSubmissionRequest">
		<xsd:sequence>
			<xsd:element name="submissionId" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="submitSubmissionRequest">
